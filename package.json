{
  "name": "strava-maps",
  "version": "0.0.1",
  "description": "Plot latests strava activities on a map overview",
  "main": "dist/index.ts",
  "repository": "https://github.com/marcelotokarnia/strava-maps",
  "author": "Marcelo Tokarnia<marcelo.tokarnia@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "^14.2.0"
  },
  "scripts": {
    "build": "tsc",
    "crawl": "node scripts/crawlLeaderboard.mjs",
    "wiki4fit": "node scripts/wiki4fit.mjs",
    "start": "node dist/index.js",
    "pretest": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx src frontend/src frontend/pages packages/strava/src packages/strava-tester/src packages/strava-parsed/src",
    "test:backend": "STRAVA_CLIENT_ID=test STRAVA_SECRET=test JEST_JUNIT_OUTPUT_NAME=test-report.xml jest",
    "test:frontend": "yarn --cwd ./frontend && CI=true yarn --cwd ./frontend test",
    "test:packages": "yarn test:package:strava && yarn test:package:strava-parsed && yarn test:package:strava-tester",
    "test:package:strava": "yarn --cwd ./packages/strava && yarn --cwd ./packages/strava test",
    "test:package:strava-parsed": "yarn --cwd ./packages/strava-parsed && yarn --cwd ./packages/strava-parsed test",
    "test:package:strava-tester": "yarn --cwd ./packages/strava-tester && yarn --cwd ./packages/strava-tester test",
    "test": "yarn test:backend && yarn test:frontend && yarn test:packages"
  },
  "husky": {
    "hooks": {
      "pre-push": "scripts/pre-push.fish"
    }
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.0",
    "@sentry/node": "^5.21.1",
    "@tokks/strava": "^1.1.5",
    "@tokks/strava-parsed": "^0.0.2",
    "@types/ioredis": "^4.27.6",
    "apollo-server-express": "^2.15.1",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-async-router": "^0.1.15",
    "graphql": "^15.3.0",
    "ioredis": "^4.27.10",
    "mappersmith": "^2.33.3",
    "module-alias": "^2.2.2",
    "ramda": "^0.27.0",
    "supertest": "^4.0.2",
    "uuid": "^8.1.0",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@tokks/strava-tester": "^0.0.3",
    "@types/express": "^4.17.3",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.9",
    "@types/ramda": "^0.27.14",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-typescript-sort-keys": "^1.2.0",
    "git-branch-is": "^4.0.0",
    "husky": "^4.2.3",
    "jest": "^26.4.0",
    "jest-junit": "^11.1.0",
    "prettier": "^2.0.1",
    "puppeteer-core": "^5.2.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.2.0",
    "typescript": "^4.0.2"
  },
  "_moduleAliases": {
    "@src": "dist"
  }
}
